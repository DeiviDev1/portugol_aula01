package aula02;

import java.util.Scanner;

public class Aula2 {

	public static void main(String[] args) {
		
         Scanner leia = new Scanner(System.in);
		 int idade;
		  
		  System.out.println("Digite sua idade: ");
		  idade = leia.nextInt();
		  
		  switch(idade) {
		  
		  case 10,11,12,13,14:
			 
			  System.out.println("infantil: ");
			  
			  break;
		  case 2:
			  
			  System.out.println("Juvenil: ");
			  
			  break;
		  case 3 :
			 
			  System.out.println("infantil: ");
			 
			  break;  
		 
			  
			  
		  }
		  
	}

}
